<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".handler.HandlerActivity">

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="subThreadSendMsgToMainClick"
        android:text="subThreadSendMsgToMain"
        android:textAllCaps="false"
        android:layout_margin="20dp"
        />

    <Button
        android:id="@+id/button15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="mainThreadSendMsgToSubClick"
        android:text="mainThreadSendMsgToSub"
        android:textAllCaps="false"
        android:layout_margin="20dp"
        />
    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="subThreadSendMsgToSubClick"
        android:text="subThreadSendMsgToSub"
        android:textAllCaps="false"
        android:layout_margin="20dp"
        />
</LinearLayout>